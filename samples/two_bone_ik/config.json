{
  "skeleton":
  {
    "filename":"robot_skeletond_unused.ozz",
    "import":
    {
      "enable":true,  // Skeleton needs to be imported/created from the source file.     "types" :
      "types":{
        "marker":true, //  Uses marker nodes as skeleton joints.
        "geometry":true, //  Uses marker nodes as skeleton joints.
        "skeleton":false //  Uses skeleton nodes as skeleton joints.
      }
    }
  },
  "animations":
  [
    {
      "filename":"robot_animation_unused",
      "tracks":
      [
        {
          "properties":  // User-channel property track must be imported.
          [
            {
              "joint_name":"target",  // Get the property from this node
              "filename":"robot_track_target.ozz",
              "property_name":"Lcl Translation",  // Name of the custom property
              "type" : 3 //  Type of the property, aka the number of floating point components. 1 to 4 components are supported.
            },
            {
              "joint_name":"aim",  // Get the property from this node
              "filename":"robot_track_aim.ozz",
              "property_name":"Lcl Translation",  // Name of the custom property
              "type" : 3 //  Type of the property, aka the number of floating point components. 1 to 4 components are supported.
            }
          ]
        }
      ]
    }
  ]
}